<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ysu.mapper.DeviceClassMapper">
    <select id="list" resultType="com.ysu.entity.DeviceClass">
        SELECT c_id,c_name  FROM device_class
    </select>
    <select id="getDeviceClass" resultType="com.ysu.entity.DeviceClass">
        SELECT c_id,c_name  FROM device_class where  c_id != 3
    </select>
</mapper>
